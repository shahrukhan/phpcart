{
    "name": "shahrukhan/phpcart",
    "description": "phpcart package for my app",
    "keywords": [
        "Cart",
        "php cart",
        "Shopping cart",
        "Laravel shopping cart",
        "Laravel cart"
    ],
    "require": {
    	"php": ">=5.4.0",
        "symfony/http-foundation": "~2.0",
        "illuminate/support": "~5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.4"
    },
    "autoload": {
        "psr-4": {
            "shahrukhan\\Phpcart\\" : "src/" 
        }
    },
    "autoload-dev": {
        "psr-4": {
            "shahrukhan\\Phpcart\\Test\\": "tests"
        }
    }
}
