{
    "name": "shahrukhan/phpcart",
    "description": "edited anam php cart for different project",
    "keywords": [
        "Cart",
        "php cart",
        "Shopping cart",
        "Laravel shopping cart",
        "Laravel cart",
        "shahrukhan"
    ],
    "homepage": "https://github.com/shahrukhan/phpcart",
    "license": "MIT",
    "authors": [
        {
            "name": "Shahrukh Khan",
            "email": "shahrukhkhan_shah@yahoo.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "symfony/http-foundation": "~3.0|~4.0",
        "illuminate/support": "~5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "Shahrukhan\\Phpcart\\" : "src/" 
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shahrukhan\\Phpcart\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shahrukhan\\Phpcart\\CartServiceProvider"
            ],
            "aliases": {
                "Cart": "Shahrukhan\\Phpcart\\Facades\\Cart"
            }
        }
    }
}